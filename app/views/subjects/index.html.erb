# coding: utf-8
<h1>Subjects</h1>

<table border="1">
  <thead>
    <tr>
      <th>sub_num</th>
      <th>sub_name</th>
      <th>credit</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subs.each do |sub| %>
      <tr>
        <td><%= sub.sub_num %></td>
        <td><%= sub.sub_num %></td>
        <td><%= sub.credit %></td>
        <td><%= link_to 'Show', subject_path(sub) %></td>
        <td><%= link_to 'Edit', edit_subject_path(sub) %></td>
        <td><%= link_to 'Destroy', subject_path(sub), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
